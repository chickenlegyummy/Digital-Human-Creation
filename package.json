{
  "name": "digital-human-creation",
  "version": "1.0.0",
  "type": "module",
  "description": "Digital Human Creation Web App with DeepSeek API",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch src/server/index.ts",
    "client:dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "keywords": [
    "digital-human",
    "ai",
    "deepseek",
    "typescript",
    "react",
    "socket.io"
  ],
  "author": "",
  "license": "MIT"
}
